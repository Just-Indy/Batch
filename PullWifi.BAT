@ECHO OFF
NETSH WLAN SHOW PROFILES
FOR /F "TOKENS=2 DELIMS=:"%%I IN ('NETSH WLAN SHOW PROFILES') DO (
SET "PROFILE=%%I"
CALL :SHOW_PROFILE !PROFILE:~1!
)
PAUSE
GOO :EOF

:SHOW_PROFILE
SETLOCAL ENABLEDELAYEDEXPANSION
SET "PROFILE=%~1"
ECHO.
ECHO PROFILE: !PROFILE!
NETSH WLAN SHOW PROFILE NAME="!PROFILE!"
KEY=CLEAR | FINDSTR /I "KEY CONTENT"
ECHO.
ENDLOCAL
GOTO :EOF